{
  "name": "aa-blog-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "generate:types": "payload generate:types",
    "standalone-script": "tsx ./src/scripts/standalone-script.ts",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.0.0-beta.18",
    "@payloadcms/email-nodemailer": "3.0.0-beta.18",
    "@payloadcms/next": "^3.0.0-beta.18",
    "@payloadcms/richtext-lexical": "^3.0.0-beta.18",
    "@payloadcms/ui": "3.0.0-beta.18",
    "@swc/core": "^1.5.0",
    "cross-env": "^7.0.3",
    "next": "^14.3.0-canary.24",
    "nodemailer": "^6.9.13",
    "payload": "^3.0.0-beta.12",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
